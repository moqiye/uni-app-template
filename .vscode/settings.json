//.vscode/settings.json

{
  // 保存时eslint自动修复错误
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  // 保存自动格式化
  "editor.formatOnSave": true,
  // 文件关联
  "files.associations": {
    "pages.json": "jsonc",
    "manifest.json": "jsonc"
  },
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false
}
